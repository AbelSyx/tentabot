<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <!-- Load parameters -->
    <rosparam command="load" 
        file="$(find tentabot)/config/tentabot_drl/drl_training_result_plot_params.yaml"
        subst_value="True" />
    
    <!-- Launch the script -->
    <node pkg="tentabot" name="plot_drl_training_result" type="plot_drl_training_result.py" output="screen"/>
</launch>